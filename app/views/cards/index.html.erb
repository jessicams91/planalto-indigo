<h1 class="ls-title-intro ls-ico-cart">Cartas</h1>
<section class="ls-form-horizontal" id="order_form">
  <div class="form-inputs">
    <label class="ls-label col-md-3">
      <%= form_tag import_cards_path, multipart: true do %>
      <%= file_field_tag :file %>
      <%= submit_tag "Importar Cartas" %>
      <% end %>
    </label>
  </div>
</section>
<hr>
<% @cards.each do |card| %>
 <tbody>
   <tr>
     <td>Nome: <%= card.name_en %> / <%= card.name_pt %></td><br>
     <td><% card_price = @prices.find_by(card: card) %></td>
     <td>Pre√ßo: R$<%= card_price.price if card_price %></td><br>
   </tr>
   <br>
</tbody>
<% end %>
